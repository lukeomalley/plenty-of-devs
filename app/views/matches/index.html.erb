<p>Current User: <%=current_user.email %></p>
<p>Liked Users:</p>
<% current_user.liked_users.each do |user| %>
  <li><%=user.email %></li>
<% end %>
<hr>

<% if @users.nil? %>
  <p>Youve liked everyone</p>
<% else %>
  <% @users.each do |user| %>
    <li><%= user.email %></li>
    <%= link_to "✅", like_user_path(user), method: :post %>
    <%= link_to "❌", dislike_user_path(user), method: :post %>
  <% end %>
<% end %>

<%= link_to "Logout", destroy_user_session_path, method: :delete %>